{
    "name": "syndesi/neo4j-sync-bundle",
    "description": "Syncs Doctrine entities to a Neo4j database",
    "version": "1.0.0-RC1",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ¶ren Klein",
            "email": "soerenklein98@gmail.com",
            "homepage": "https://syndesi.dev",
            "role": "Developer"
        }
    ],
    "keywords": [
        "graph",
        "database",
        "neo4j",
        "symfony",
        "doctrine",
        "sync"
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "laudis/neo4j-php-client": "^2.5",
        "symfony/yaml": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/monolog-bundle": "^3.7",
        "psalm/attributes": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Syndesi\\Neo4jSyncBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syndesi\\Neo4jSyncBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "symfony/phpunit-bridge": "^6.0",
        "psalm/plugin-phpunit": "^0.16.1",
        "vimeo/psalm": "^4.17"
    },
    "scripts": {
        "test": "php ./vendor/phpunit/phpunit/phpunit",
        "cs:list": "php vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --dry-run ./src",
        "cs:fix": "php vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./src"
    }
}
